<html>
  <head>
    <link rel='stylesheet' type='text/css' href='styles/main.css'
  </head>

  <body>
    <button id='button-ping'>Ping!</button>
    <p id='out-pong'></p>

    <!--Login-->
    <div id='login-wrap'>
      <p id='login-error-display'>&nbsp;</p>
      <form id='send-login'>
          <input size='32' maxLength='32' id='username-box' placeholder='username'/>
          <input type='submit' />
      </form>
    </div>

    <!--Chat-->
    <div id='chat-wrap'>
      <div class='chat-log-display' id='chat-display'></div>
      <form id='send-message'>
        <input size='64' maxLength= '128' id='message-box' placeholder='message' />
        <input type='submit' />
      </form>
    </div>

    <!--Userlist-->
    <div id='userlist-wrap'>
      <p><b>Userlist</b></p>
      <div id='userlist'></div>
    </div>

    <!--Dependencies-->
    <script src='bower_components/jQuery/dist/jquery.js'></script>
    <script src='bower_components/bootstrap/dist/js/bootstrap.js'></script>
    <script src='/socket.io/socket.io.js'></script>

    <!--scripts-->
    <script src='/scripts/ping-pong.js'></script>
    <script src='/scripts/chat.js'></script>
    <script src='/scripts/rp/html/DynamicDiv.js'></script>
    <script src='/scripts/rp/html/ListDiv.js'></script>
  </body>
</html>
