<html>
  <head>
  </head>

  <body>
    <button id='button-ping'>Ping!</button>
    <p id='out-pong'></p>

    <!--Chat-->
    <div class='chat-log-display' id='chat'></div>
    <form id='send-message'>
      <input size='16' maxLength='16' id='username-box' placeholder='username'/>
      <input size='64' maxLength= '128' id='message-box' placeholder='message' />
      <input type='submit' />
    </form>

    <!--Dependencies-->
    <script src='https://code.jquery.com/jquery-2.1.3.min.js'></script>
    <script src='/socket.io/socket.io.js'></script>

    <!--scripts-->
    <script src='/scripts/ping-pong.js'></script>
  </body>
</html>
